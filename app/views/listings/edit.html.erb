<div class="Edit">
    
    <td class="center"><%= @listing.description %></td>
    <% form_for @listing do |f| %> 
    
  <p>
   <%= f.label :name, "name" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
        <%= f.label :description %><br />
        <%= f.text_field :description %>
  </p>
  <p>
        <%= f.label :price %><br />
        <%= f.text_area :price %>
  </p>
  <p>
        <%= f.submit "Update" %>
  </p>



 <% end %>
</div>
