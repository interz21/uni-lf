<% if !flash[:notice].blank? %>
  <div class="notice">
      <%= flash[:notice] %>
  </div>
<% end %>

<div class ="photo">
  <%= image_tag(@avatar)%>
  <p> Print thing <%= puts @avatar %></p>
</div>

<div class="listings index">
  <h2>Listings</h2>

  <%= link_to("Add new listing", {:action => 'new'}, :class => 'action new') %>

  <table class="listing" summary="Listing list">
    <tr class="header">
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Posted</th>
    </tr>
    <% @user_listings.each do |listing| %>
    <tr>
      <td><%= listing.name %></td>
      <td class="center"><%= listing.description %></td>
      <td class="center"><%= listing.price %></td>
      <td class="center"><%= listing.created_at %></td>
      <td class="actions">
        <%= link_to("Show", {:action => 'show', :id => listing.id}, :class => 'action show')%>
        <%= link_to("Edit", {:action => 'edit', :id => listing.id}, :class => 'action edit')%>
        <%= link_to("Delete", {:action => 'delete', :id => listing.id}, :class => 'action delete')%>
      </td>
    </tr>
    <% end %>
  </table>

  <br />


</div>
